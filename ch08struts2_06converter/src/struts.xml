<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="scores">                                          
			<result>/scores.jsp</result>
		</action>
		
		<action name="showscores" class="actions.ScoreAction"> 
			<result name="success">/showScores.jsp</result>
		</action>
		
		<action name="cellphone">                                          
			<result>/cellPhone.jsp</result>
		</action>
		
		<action name="showscellphone" class="actions.CellPhoneAction"> 
			<result name="success">/showCellPhone.jsp</result>
			<result name = "input">/cellPhone.jsp</result>
		</action>
		
	</package>	
</struts>    
